<%= form_for ([@car, @booking]) do |f| %>
<%= render 'shared/error_messages', object: f.object %>
  <p>
    <%= f.label :start_time %>
    <%= f.datetime_select :start_time, { minute_step: 15 } %>
  </p>
  <p>
    <%= f.label :number_of_days %>
    <%= f.number_field :length, min: 1 %>
  </p>
  <%= f.submit "Book it" %>
<% end %> 



